{-# LANGUAGE ScopedTypeVariables #-}
module HList3 where
import Data.DList

--Super contrived example
explode :: Int -> DList a -> [a]
explode n lst = concat (map (\x -> replicate n x) (toList lst))

f :: IO ()
f = do
  (lst :: [Int]) <- read <$> getLine
  let newLst = explode 3 (fromList lst)
  print newLst
